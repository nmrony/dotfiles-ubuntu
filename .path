
export RVM_PATH="$HOME/.rvm/bin"
export PATH="$RVM_PATH:$PATH" # Add RVM to PATH for scripting
export GO_PATH=/usr/local/opt/go/libexec/bin
export GOPATH="$HOME/.go"
export PATH="$PATH:$GO_PATH:$GOPATH/bin"
export PATH="/usr/local/sbin:$PATH"
export CDPATH=${CDPATH}:$GOPATH/src/github.com:$GOPATH/src/golang.org:$GOPATH/src
